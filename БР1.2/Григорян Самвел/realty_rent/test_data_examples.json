  "validation_rules": {
    "user": {
      "username": {
        "required": true,
        "min_length": 3,
        "max_length": 150,
        "pattern": "^[a-zA-Z0-9_]+$"
      },
      "email": {
        "required": true,
        "format": "email",
        "max_length": 254
      },
      "password": {
        "required": true,
        "min_length": 8
      },
      "Phone": {
        "format": "^\\+7[0-9]{10}$"
      },
      "Passport": {
        "format": "^[0-9]{10}$"
      }
    },
    "building": {
      "City": {
        "required": true,
        "max_length": 100
      },
      "Street": {
        "required": true,
        "max_length": 100
      },
      "Number": {
        "required": true,
        "max_length": 100
      }
    },
    "apartment": {
      "Number": {
        "required": true,
        "type": "integer",
        "min": 1
      },
      "Square": {
        "required": true,
        "type": "integer",
        "min": 1
      },
      "Cost": {
        "required": true,
        "type": "integer",
        "min": 0
      },
      "BuildingID": {
        "required": true,
        "type": "integer",
        "exists": "buildings"
      }
    },
    "contract": {
      "AgentID": {
        "required": true,
        "type": "integer",
        "exists": "users"
      },
      "ClientID": {
        "required": true,
        "type": "integer",
        "exists": "users"
      },
      "ApartmentID": {
        "required": true,
        "type": "integer",
        "exists": "apartments"
      },
      "Status": {
        "enum": ["v", "l", "f"]
      },
      "startDate": {
        "format": "date"
      },
      "endDate": {
        "format": "date"
      }
    }
  }
